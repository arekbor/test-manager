{% extends 'testSolve/base.html.twig' %}

{% block wrapper %}
    {% if not clause() %}
        <div class="alert alert-warning">
            <small>
                {{ 'templates.testSolve.clause.notFoundMessage'|trans({ 
                    '%language%': app.request.getLocale()|trans|lower
                }) }}
            </small>
        </div>
    {% else %}
        <div class="alert alert-info">
            {{ clause() }}
        </div>
    {% endif %}
{% endblock %}