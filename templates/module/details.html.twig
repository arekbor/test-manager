{% extends 'base.html.twig' %}

{% block title %} 
    {{ 'title.module.details'|trans }} 
{% endblock %}

{% block body %}
    <div class="mt-2">
        {{ data_table(questions) }}
    </div>
    
    <div class="mt-3">
        {{ form_start(form) }}
            {{ form_row(form.name) }}
            {{ form_row(form.language) }}

            <div class="mt-2">
                {{ form_row(form.submit) }}
            </div>

        {{ form_end(form) }}
    </div>
{% endblock %}
