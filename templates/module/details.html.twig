{% extends 'base.html.twig' %}

{% block title %} 
    {{ 'title.module.details'|trans }} 
{% endblock %}

{% block body %}
    <div class="mt-2">
        {{ component('HeaderBreakLine', { title: 'question.data.table.title'|trans, defer: true }) }}
        {{ data_table_action_bar(questions) }}
        {{ data_table_table(questions) }}
        <div class="mt-3">
            {{ data_table_pagination(questions) }}
        </div>
    </div>

    <div class="mt-5">
        {{ component('HeaderBreakLine', { title: 'video.data.table.title'|trans, defer: true }) }}
        {{ data_table_action_bar(videos) }}
        {{ data_table_table(videos) }}
    </div>

    <div class="mt-5">
        {{ component('ModuleForm', { moduleProp: module, defer: true }) }}
    </div>
{% endblock %}
