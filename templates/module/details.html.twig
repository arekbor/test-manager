{% extends 'base.html.twig' %}

{% block title %} 
    {{ 'title.module.details'|trans }} 
{% endblock %}

{% block body %}
    <div class="mt-2">
        <twig:TableTitle>
            {{ 'question.data.table.title'|trans }}
        </twig:TableTitle>
        
        {{ data_table_action_bar(questions) }}
        {{ data_table_table(questions) }}
        <div class="mt-3">
            {{ data_table_pagination(questions) }}
        </div>
    </div>
    
    {{ component('ModuleForm', { moduleId: moduleId, defer: true }) }}
{% endblock %}
